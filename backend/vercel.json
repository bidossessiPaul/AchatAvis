{
    "version": 2,
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/app.ts"
        },
        {
            "src": "/health",
            "dest": "src/app.ts"
        },
        {
            "src": "/public/(.*)",
            "dest": "public/$1"
        }
    ]
}